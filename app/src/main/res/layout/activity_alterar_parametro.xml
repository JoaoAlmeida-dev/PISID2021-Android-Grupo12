                        android:layout_weight="1"
                        android:ems="8"
                        android:gravity="center"
                        android:hint="DangerZoneMaxHumidade"
                        android:inputType="numberDecimal"
                        android:textSize="12sp" />
                </TableRow>

            </TableLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/DangerZoneMaxTemperaturaText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="DangerZoneMinHumidade" />

                    <TextView
                        android:id="@+id/DangerZoneMinTemperaturaText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="DangerZoneMaxHumidade" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <EditText
                        android:id="@+id/DangerZoneMaxTemperatura"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ems="8"
                        android:gravity="center"
                        android:hint="DangerZoneMaxTemperatura"
                        android:inputType="numberDecimal"
                        android:textSize="12sp" />

                    <EditText
                        android:id="@+id/DangerZoneMinTemperatura"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ems="8"
                        android:gravity="center"
                        android:hint="DangerZoneMinTemperatura"
                        android:inputType="numberDecimal"
                        android:textSize="12sp" />
                </TableRow>

            </TableLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/DangerZoneMinLuzText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="DangerZoneMinHumidade" />

                    <TextView
                        android:id="@+id/DangerZoneMaxLuzText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="DangerZoneMaxHumidade" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    numberDecimal<EditText
                        android:id="@+id/DangerZoneMinLuz"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ems="8"
                        android:gravity="center"
                        android:hint="DangerZoneMinLuz"
                        android:inputType="numberDecimal"
                        android:textSize="12sp" />

                    <EditText
